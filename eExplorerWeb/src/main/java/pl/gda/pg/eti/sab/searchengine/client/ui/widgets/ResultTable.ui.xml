<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'
			 xmlns:cellview="urn:import:com.google.gwt.user.cellview.client">

	<ui:style>
		.resultPanel {
			margin: auto;
		}

		.resultTable {
			width: 960px;
			text-align: left;
		}
	</ui:style>

	<g:VerticalPanel>
		<g:cell horizontalAlignment="ALIGN_CENTER">
			<cellview:CellTable ui:field="resultTable" styleName="{style.resultTable}"/>
		</g:cell>
		<g:cell horizontalAlignment="ALIGN_CENTER">
			<cellview:SimplePager ui:field="resultPager"/>
		</g:cell>
	</g:VerticalPanel>
</ui:UiBinder>